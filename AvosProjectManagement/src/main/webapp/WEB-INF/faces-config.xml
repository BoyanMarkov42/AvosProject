<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">

<managed-bean eager="true">
	<managed-bean-name>allUsersTableInfo</managed-bean-name>
	<managed-bean-class>com.info.AllUsersTableInfo</managed-bean-class> 
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean> 
<managed-bean eager="true">
	<managed-bean-name>allUsersBean</managed-bean-name>
	<managed-bean-class>com.beans.AllUsersBean</managed-bean-class> 
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean eager="true">
	<managed-bean-name>oneUserTableInfo</managed-bean-name>
	<managed-bean-class>com.info.OneUserTableInfo</managed-bean-class> 
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean> 
<managed-bean eager="true">
	<managed-bean-name>oneUserBean</managed-bean-name>
	<managed-bean-class>com.beans.OneUserBean</managed-bean-class> 
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

</faces-config>
